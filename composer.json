{
	"name": "regulus/fractal",
	"description": "A simple, versatile CMS base for Laravel.",
	"license": "MIT",
	"authors": [
		{
			"name": "Cody Jassman",
			"email": "me@codyjassman.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"illuminate/support": "4.2.*|5.*",
		"regulus/activity-log": "0.3.1",
		"regulus/elemental": "0.3.3",
		"regulus/formation": "@dev",
		"regulus/solid-site": "0.5.0",
		"regulus/tetra-text": "0.4.4",
		"regulus/upstream": "0.4.4",
		"maxhoffmann/parsedown-laravel": "@dev",
		"julien-c/simple-resize": "@dev"
	},
	"autoload": {
		"classmap": [
			"src/commands",
			"src/controllers",
			"src/libraries",
			"src/migrations",
			"src/models",
			"src/seeds",
			"src/traits"
		],
		"psr-0": {
			"Regulus\\Fractal": "src/"
		}
	},
	"minimum-stability": "stable"
}