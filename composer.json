{
	"name": "regulus/fractal",
	"description": "A simple, versatile CMS base for Laravel 4 which uses Twitter Bootstrap.",
	"license": "MIT",
	"authors": [
		{
			"name": "Cody Jassman",
			"email": "me@codyjassman.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"illuminate/support": "4.0.x",
		"regulus/activity-log": "dev-master",
		"regulus/solid-site": "dev-master",
		"regulus/tetra-text": "dev-master",
		"aquanode/formation": "dev-master",
		"aquanode/elemental": "dev-master",
		"aquanode/upstream": "dev-master"
	},
	"autoload": {
		"classmap": [
			"src/commands",
			"src/controllers",
			"src/migrations",
			"src/models",
			"src/seeds"
		],
		"psr-0": {
			"Regulus\\Fractal": "src/"
		}
	},
	"minimum-stability": "dev"
}